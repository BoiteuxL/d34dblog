{
  "name": "d34dblog",
  "version": "1.0.0",
  "description": "A small blog and personal website I'm working on",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch --poll src/assets/scss:dist/assets/css",
    "build:sass": "sass src/assets/scss/styles.scss:dist/assets/css/styles.css --style=compressed --no-source-map",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoiteuxL/d34dblog.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BoiteuxL/d34dblog/issues"
  },
  "homepage": "https://github.com/BoiteuxL/d34dblog#readme",
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "@tailwindcss/postcss": "^4.1.10",
    "cssnano": "^7.0.7",
    "flowbite": "^3.1.2",
    "markdown-it-link-attributes": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "css-minify": "^2.1.0",
    "daisyui": "^5.0.43",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10"
  }
}
